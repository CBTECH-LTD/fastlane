{
    "name": "cbtech-ltd/fastlane",
    "description": "Fastlane is a toolkit to build custom CMS.",
    "keywords": [
        "cbtech-ltd",
        "fastlane"
    ],
    "homepage": "https://github.com/cbtech-ltd/fastlane",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Vanderlei Sbaraini Amancio",
            "email": "vanderlei@cbtech.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "altek/accountant": "^3.0.1",
        "altek/eventually": "^2.0",
        "hashids/hashids": "^4.0",
        "illuminate/support": "^8.0",
        "laravel/sanctum": "^2.7",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "spatie/laravel-permission": "^3.17",
        "spatie/laravel-view-models": "^1.3",
        "webmozart/assert": "^1.8.0",
        "livewire/livewire": "^2.3.0",
        "spatie/once": "^2.2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.2",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.17.1"
    },
    "autoload": {
        "psr-4": {
            "CbtechLtd\\Fastlane\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CbtechLtd\\Fastlane\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --color=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "post-autoload-dump": [
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CbtechLtd\\Fastlane\\Providers\\FastlaneServiceProvider"
            ]
        }
    }
}
